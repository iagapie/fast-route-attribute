{
    "name": "iagapie/fast-route-attribute",
    "type": "library",
    "description": "FastRoute - PHP 8 Attribute",
    "keywords": [
        "routing",
        "router",
        "fast-route",
        "attribute"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Igor Agapie",
            "email": "igoragapie@gmail.com"
        }
    ],
    "require": {
        "php": ">= 8.0",
        "nikic/fast-route": "^1.3",
        "psr/http-server-middleware": "^1.0",
        "symfony/finder": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "IA\\Route\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IA\\Route\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
